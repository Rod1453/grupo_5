<%- include('../partials/headParte1') %>
    <!--Aqui se agregan los estilos personales-->
    <link rel="stylesheet" href="/css/profile.css" />

    <%- include('../partials/headParte2') %>

        <%- include('../partials/header') %>

            <main class="container">
                <div class="container-foto">
                    <img id="foto-perfil" src="/img/users/<%=user.imagen %>" alt="img">
                </div>
                <div class="datos">
                    <h2 class="username"><%=user.email %></h2>
                    <br>
                    <p>Nombre</p>
                    <div class="campo">
                        <div><%=user.nombre %></div>
                    </div>
                    <p>Apellido</p>
                    <div class="campo">
                        <div><%=user.apellido %></div>
                    </div>
                    <p>Categoria</p>
                    <div class="campo">
                        <div><%=user.categoria %></div>
                    </div>
                    <a href="/user/edit/<%= user.id %>">Editar</a>
                </div>
            </main>

            <%- include('../partials/footer') %>